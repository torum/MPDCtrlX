<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
			 xmlns:vm="clr-namespace:MPDCtrlX.ViewModels"
			 xmlns:prop="clr-namespace:MPDCtrlX.Properties"
             x:Class="MPDCtrlX.Views.Dialogs.RenameNewPlaylistDialog">
	<Grid>
		<StackPanel Margin="10" Width="250">
			<TextBox Name="TextBoxPlaylistName" Margin="10" HorizontalAlignment="Stretch" VerticalAlignment="Top" Watermark="{x:Static prop:Resources.Dialog_TextBox_Watermark_EnterNewRenamedPlaylistName}" AttachedToVisualTree="InputField_OnAttachedToVisualTree" />
		</StackPanel>
	</Grid>
</UserControl>
